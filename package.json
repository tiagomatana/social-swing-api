{
  "name": "social-swing-api",
  "version": "1.0.0",
  "main": "src/server.ts",
  "description": "API for social swing app",
  "repository": "git@github.com:tiagomatana/social-swing-api.git",
  "author": "Tiago Matana <tiago.matana@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev": "tsnd -r tsconfig-paths/register --transpile-only --ignore-watch node_modules src/server.ts",
    "typeorm": "tsnd ./node_modules/typeorm/cli.js",
    "start": "node dist/server.js",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist && gulp copy-html",
    "test": "jest --forceExit --runInBand --detectOpenHandles --collectCoverage",
    "test:dev": "jest --runInBand --detectOpenHandles --forceExit",
    "reporter": "jest --json --outputFile=result.json --testLocationInResults --forceExit",
    "test-coverage": "node_modules/.bin/jest --watchAll --runInBand --verbose --coverage"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {

    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "del": "^6.0.0",
    "dotenv": "^8.2.0",
    "email-validator": "^2.0.4",
    "enum": "^3.0.4",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-list-endpoints": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.2",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.14",
    "pg": "^8.4.1",
    "socket.io": "^3.0.3",
    "typeorm": "^0.2.28",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.8",
    "@types/del": "^4.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/express-list-endpoints": "^4.0.1",
    "@types/jest": "^25.1.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.4",
    "@types/nodemailer": "^6.4.0",
    "@types/sinon": "^9.0.8",
    "@types/socket.io": "^2.1.11",
    "@types/supertest": "^2.0.10",
    "@types/yup": "^0.29.8",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "express-server-utils": "^1.1.0",
    "gulp": "^4.0.2",
    "jest": "^26.6.0",
    "jest-extended": "^0.11.5",
    "mock-express-request": "^0.2.2",
    "mock-express-response": "^0.2.2",
    "sino": "^0.0.3",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-node-dev": "^1.0.0-pre.63",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.0.3"
  }
}
